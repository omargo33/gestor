<?xml version="1.0" encoding="UTF-8" ?><adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">    <view id="Home">        <page>/Home.jspx</page>    </view>    <method-call id="base_obtenerParametroTexto01">        <method>#{bindings.base_obtenerParametroTexto01.execute}</method>        <return-value>#{sessionScope.urlInformacion}</return-value>                <outcome>            <fixed-outcome>base_obtenerParametroTexto01</fixed-outcome>        </outcome>    </method-call>    <url-view id="urlView1">        <url>#{sessionScope.urlInformacion}</url>    </url-view>    <control-flow-rule id="__1">        <from-activity-id>Home</from-activity-id>        <control-flow-case id="__2">            <from-outcome>informacion</from-outcome>            <to-activity-id>base_obtenerParametroTexto01</to-activity-id>        </control-flow-case>    </control-flow-rule>    <control-flow-rule id="__3">        <from-activity-id>base_obtenerParametroTexto01</from-activity-id>        <control-flow-case id="__4">            <from-outcome>base_obtenerParametroTexto01</from-outcome>            <to-activity-id>urlView1</to-activity-id>        </control-flow-case>    </control-flow-rule></adfc-config>
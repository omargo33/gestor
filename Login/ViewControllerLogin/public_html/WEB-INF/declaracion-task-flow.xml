<?xml version="1.0" encoding="UTF-8" ?><adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">  <task-flow-definition id="declaracion-task-flow">    <default-activity>base_obtenerParametroNumerico01</default-activity>    <data-control-scope>      <shared/>    </data-control-scope>    <method-call id="base_obtenerParametroTexto01">      <method>#{bindings.base_obtenerParametroTexto01.execute}</method>      <return-value>#{sessionScope.urlDeclaimer}</return-value>      <outcome>        <fixed-outcome>base_obtenerParametroTexto01</fixed-outcome>      </outcome>    </method-call>    <view id="declaimer">      <page>/declaimer/declaimer.jsff</page>    </view>    <method-call id="base_obtenerParametroNumerico01">      <method>#{bindings.base_obtenerParametroNumerico01.execute}</method>      <return-value>#{sessionScope.declaimer}</return-value>      <outcome>        <fixed-outcome>base_obtenerParametroNumerico01</fixed-outcome>      </outcome>    </method-call>    <router id="router1">      <case id="__8">        <expression>#{sessionScope.declaimer == 0}</expression>        <outcome>vacio</outcome>      </case>      <default-outcome>declaimer</default-outcome>    </router>    <control-flow-rule id="__1">      <from-activity-id>base_obtenerParametroTexto01</from-activity-id>      <control-flow-case id="__2">        <from-outcome>base_obtenerParametroTexto01</from-outcome>        <to-activity-id>declaimer</to-activity-id>      </control-flow-case>    </control-flow-rule>    <control-flow-rule id="__3">      <from-activity-id>base_obtenerParametroNumerico01</from-activity-id>      <control-flow-case id="__4">        <from-outcome>base_obtenerParametroNumerico01</from-outcome>        <to-activity-id>router1</to-activity-id>      </control-flow-case>    </control-flow-rule>    <control-flow-rule id="__5">      <from-activity-id>router1</from-activity-id>      <control-flow-case id="__6">        <from-outcome>vacio</from-outcome>        <to-activity-id>declaimer</to-activity-id>      </control-flow-case>      <control-flow-case id="__7">        <from-outcome>declaimer</from-outcome>        <to-activity-id>base_obtenerParametroTexto01</to-activity-id>      </control-flow-case>    </control-flow-rule>    <use-page-fragments/>  </task-flow-definition></adfc-config>
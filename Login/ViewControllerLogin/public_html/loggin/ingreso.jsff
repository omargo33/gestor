<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core">    <af:panelGroupLayout id="pgl2" layout="vertical" halign="center" valign="middle">        <af:outputFormatted value="#{bnd.loggin_ingreso_titulo}" id="of1"                            inlineStyle="font-weight:bolder; font-size:large;"/>        <af:spacer id="s2" height="18" width="18"/>        <af:panelGroupLayout id="pgl5" layout="vertical" halign="center">            <af:panelFormLayout id="pfl1" labelAlignment="top">                <af:inputText value="#{bindings.nick.inputValue}" required="true" columns="20"                              maximumLength="#{bindings.nick.hints.precision}"                              shortDesc="#{bindings.nick.hints.tooltip}" id="it1" labelStyle="font-weight:bolder; "                              contentStyle="font-weight:bolder; font-size:medium;"                              label="#{bnd.loggin_ingreso_usuario}">                    <f:validator binding="#{bindings.nick.validator}"/>                </af:inputText>                <af:spacer id="s4" height="10" width="10"/>                <af:inputText value="#{bindings.password.inputValue}" required="true" columns="20"                              maximumLength="#{bindings.password.hints.precision}"                              shortDesc="#{bindings.password.hints.tooltip}" id="it2" labelStyle="font-weight:bolder;"                              contentStyle="font-weight:bolder; font-size:medium;" secret="true"                              label="#{bnd.loggin_ingreso_password}">                    <f:validator binding="#{bindings.password.validator}"/>                </af:inputText>            </af:panelFormLayout>            <f:facet name="separator"/>        </af:panelGroupLayout>        <af:spacer id="s3" height="13" width="13"/>        <af:button text="#{bnd.btn_entrar}"                   disabled="#{!bindings.validarUsuario.enabled}" id="b2" action="#{BackingIngreso.actionIngresar}"                   icon="../../Imagen-001/icons/7548937_keys_ui_security_user_interface_key_lock.svg"/>        <f:facet name="separator"/>        <af:spacer id="s1" height="25" width="25"/>        <af:link text="#{bnd.btn_clave_perdida}" id="l1" inlineStyle="font-weight:bolder; font-size:small;"                 action="solicitar" immediate="true">            <af:resetActionListener/>        </af:link>    </af:panelGroupLayout></jsp:root>
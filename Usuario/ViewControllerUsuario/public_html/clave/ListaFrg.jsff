<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core">    <af:pageTemplate viewId="/plantilla/listaFrg.jspx" id="pt1">        <f:facet name="elementoBusqueda"/>        <f:facet name="elementoCentro">            <af:panelFormLayout id="pfl1" labelWidth="140">                <af:inputText value="#{bindings.passwordOld.inputValue}" label="#{bnd.cambiar_clave_clave_anterior}"                              required="true"                              columns="#{bindings.passwordOld.hints.displayWidth}"                              maximumLength="#{bindings.passwordOld.hints.precision}"                              shortDesc="#{bindings.passwordOld.hints.tooltip}" id="it1" secret="true"                              showRequired="true">                    <f:validator binding="#{bindings.passwordOld.validator}"/>                </af:inputText>                <af:inputText value="#{bindings.passwordNew.inputValue}" label="#{bnd.cambiar_clave_clave_nueva}"                              required="true"                              columns="#{bindings.passwordNew.hints.displayWidth}"                              maximumLength="#{bindings.passwordNew.hints.precision}"                              shortDesc="#{bindings.passwordNew.hints.tooltip}" id="it2" secret="true"                              showRequired="true">                    <f:validator binding="#{bindings.passwordNew.validator}"/>                </af:inputText>                <af:inputText value="#{bindings.passwordConfirmar.inputValue}"                              label="#{bnd.cambiar_clave_clave_repetir}"                              required="true"                              columns="#{bindings.passwordConfirmar.hints.displayWidth}"                              maximumLength="#{bindings.passwordConfirmar.hints.precision}"                              shortDesc="#{bindings.passwordConfirmar.hints.tooltip}" id="it3" secret="true"                              showRequired="true">                    <f:validator binding="#{bindings.passwordConfirmar.validator}"/>                </af:inputText>                <f:facet name="footer">                    <af:button                                text="#{bnd.cambiar_clave_cambiar_clave}"                               disabled="#{!bindings.cambiarClaveCompleto.enabled}" id="b1"                               action="#{BackingListaFrg.cambiarClave}"/>                </f:facet>            </af:panelFormLayout>        </f:facet>        <f:facet name="elementoAcciones"/>        <f:attribute name="titulo" value="#{bnd.cambiar_clave_listaFrg_titulo}"/>        <f:attribute name="tituloDescripcion" value="#{bnd.cambiar_clave_listaFrg_descripcion}"/>    </af:pageTemplate></jsp:root>
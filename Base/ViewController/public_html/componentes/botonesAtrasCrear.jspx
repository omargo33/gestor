<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:f="http://java.sun.com/jsf/core"          xmlns:af="http://xmlns.oracle.com/adf/faces/rich">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <af:componentDef var="attrs" componentVar="comp" definition="private">        <af:xmlContent>            <component xmlns="http://xmlns.oracle.com/adf/faces/rich/component">                <description>Botones de Atras y Crear</description>                <display-name>botonesAtrasCrear</display-name>                <component-class>base.botones.BotonesAtrasCrear</component-class>                <facet>                    <description>Botones de acciones varias</description>                    <facet-name>botonesAcciones</facet-name>                </facet>                <attribute>                    <attribute-name>crearTexto</attribute-name>                    <attribute-class>java.lang.String</attribute-class>                </attribute>                <attribute>                    <attribute-name>crearPermiso</attribute-name>                    <attribute-class>java.lang.Boolean</attribute-class>                    <default-value>                        true                    </default-value>                </attribute>                <component-extension>                    <component-tag-namespace>base.botones</component-tag-namespace>                    <component-taglib-uri>/baseLib01</component-taglib-uri>                    <method-attribute>                        <attribute-name>crearAccion</attribute-name>                        <method-signature>java.lang.String method()</method-signature>                    </method-attribute>                    <method-attribute>                        <attribute-name>atrasAccion</attribute-name>                        <method-signature>java.lang.String method()</method-signature>                    </method-attribute>                </component-extension>            </component>        </af:xmlContent>        <af:panelGroupLayout id="pgl3" layout="horizontal">                                <af:facetRef facetName="botonesAcciones"/>            <af:button text="#{attrs.crearTexto}" id="pt_b1" action="#{comp.handleCrearAccion}"                       icon="../../Imagen-001/icons/3994437_create_positive_add_plus_new.svg" rendered="#{attrs.crearPermiso}"/>            <f:facet name="separator">                <af:spacer id="s1" height="3" width="3"/>            </f:facet>        </af:panelGroupLayout>    </af:componentDef></jsp:root>
<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <af:componentDef var="attrs" componentVar="comp" definition="private">        <af:xmlContent>            <component xmlns="http://xmlns.oracle.com/adf/faces/rich/component">                <description>Botones de Guardar, Borrar y PopUp de Borrado</description>                <display-name>botonesGuardarBorrar</display-name>                <component-class>base.botones.BotonesGuardarBorrar</component-class>                <attribute>                    <attribute-name>actualizarPermiso</attribute-name>                    <attribute-class>java.lang.Boolean</attribute-class>                    <default-value>0</default-value>                </attribute>                <attribute>                    <attribute-name>borrarPermiso</attribute-name>                    <attribute-class>java.lang.Boolean</attribute-class>                    <default-value>0</default-value>                </attribute>                <attribute>                    <attribute-name>nivel</attribute-name>                    <attribute-class>java.lang.Integer</attribute-class>                    <default-value>1</default-value>                </attribute>                <attribute>                    <attribute-name>borrarDisabled</attribute-name>                    <attribute-class>java.lang.Boolean</attribute-class>                    <default-value>0</default-value>                </attribute>                <component-extension>                    <component-tag-namespace>base.botones</component-tag-namespace>                    <component-taglib-uri>/baseLib01</component-taglib-uri>                </component-extension>                <component-extension>                    <component-tag-namespace>base.botones</component-tag-namespace>                    <component-taglib-uri>/baseLib01</component-taglib-uri>                    <method-attribute>                        <attribute-name>actualizarAccion</attribute-name>                        <method-signature>java.lang.String method()</method-signature>                    </method-attribute>                    <method-attribute>                        <attribute-name>borrarAccion</attribute-name>                        <method-signature>java.lang.String method()</method-signature>                    </method-attribute>                </component-extension>            </component>        </af:xmlContent>        <af:resource type="javascript">          //P1          function hidePopupP1() {              var popup = AdfPage.PAGE.findComponent("p1::content");              popup.hide();          }          //P2          function hidePopupP2() {              var popup = AdfPage.PAGE.findComponent("p2::content");              popup.hide();          }          //P3          function hidePopupP3() {              var popup = AdfPage.PAGE.findComponent("p3::content");              popup.hide();          }          //P4          function hidePopupP4() {              var popup = AdfPage.PAGE.findComponent("p4::content");              popup.hide();          }        </af:resource>        <af:panelGroupLayout id="pgl0">            <af:button text="#{bndBase.btn_guardar}" id="b0" action="#{comp.handleActualizarAccion}"                       rendered="#{attrs.actualizarPermiso}"/>            <af:group id="gr1" rendered="#{attrs.borrarPermiso}">                <!-- nivel 1 -->                <af:button text="#{bndBase.btn_borrar}" id="b1" disabled="#{attrs.borrarDisabled}"                           rendered="#{attrs.nivel eq 1}" partialTriggers="b0">                    <af:showPopupBehavior popupId="p1"/>                </af:button>                <!-- nivel 2 -->                <af:button text="#{bndBase.btn_borrar}" id="b2" disabled="#{attrs.borrarDisabled}"                           rendered="#{attrs.nivel eq 2}" partialTriggers="b0">                    <af:showPopupBehavior popupId="p2"/>                </af:button>                <!-- nivel 3 -->                <af:button text="#{bndBase.btn_borrar}" id="b3" disabled="#{attrs.borrarDisabled}"                           rendered="#{attrs.nivel eq 3}" partialTriggers="b0">                    <af:showPopupBehavior popupId="p3"/>                </af:button>                <!-- nivel 4 -->                <af:button text="#{bndBase.btn_borrar}" id="b4" disabled="#{attrs.borrarDisabled}"                           rendered="#{attrs.nivel eq 4}" partialTriggers="b0">                    <af:showPopupBehavior popupId="p4"/>                </af:button>            </af:group>            <f:facet name="separator">                <af:spacer id="s3" height="3" width="3"/>            </f:facet>        </af:panelGroupLayout>        <!-- nivel 1 -->        <af:popup childCreation="deferred" autoCancel="disabled" id="p1" rendered="#{attrs.nivel eq 1}">            <af:dialog id="d1" type="cancel" title="#{bndBase.msg_borrar_titulo}">                <f:facet name="buttonBar">                    <af:button id="cb1" text="#{bndBase.btn_borrar}" action="#{comp.handleBorrarAccion}">                        <af:clientListener method="hidePopupP1" type="action"/>                    </af:button>                </f:facet>                <af:panelBorderLayout id="pbl1">                    <af:panelHeader id="pgl1" text="#{bndBase.msg_borrar_advertencia}" messageType="warning">                        <af:outputText value="#{bndBase.msg_borrar_mensaje}" id="ot1"/>                    </af:panelHeader>                </af:panelBorderLayout>            </af:dialog>        </af:popup>        <!-- nivel 2 -->        <af:popup childCreation="deferred" autoCancel="disabled" id="p2" rendered="#{attrs.nivel eq 2}">            <af:dialog id="d2" type="cancel" title="#{bndBase.msg_borrar_titulo}">                <f:facet name="buttonBar">                    <af:button id="cb2" text="#{bndBase.btn_borrar}" action="#{comp.handleBorrarAccion}">                        <af:clientListener method="hidePopupP2" type="action"/>                    </af:button>                </f:facet>                <af:panelBorderLayout id="pbl2">                    <af:panelHeader id="pgl2" text="#{bndBase.msg_borrar_advertencia}" messageType="warning">                        <af:outputText value="#{bndBase.msg_borrar_mensaje}" id="ot2"/>                    </af:panelHeader>                </af:panelBorderLayout>            </af:dialog>        </af:popup>        <!-- nivel 3 -->        <af:popup childCreation="deferred" autoCancel="disabled" id="p3" rendered="#{attrs.nivel eq 3}">            <af:dialog id="d3" type="cancel" title="#{bndBase.msg_borrar_titulo}">                <f:facet name="buttonBar">                    <af:button id="cb3" text="#{bndBase.btn_borrar}" action="#{comp.handleBorrarAccion}">                        <af:clientListener method="hidePopupP3" type="action"/>                    </af:button>                </f:facet>                <af:panelBorderLayout id="pbl3">                    <af:panelHeader id="pgl3" text="#{bndBase.msg_borrar_advertencia}" messageType="warning">                        <af:outputText value="#{bndBase.msg_borrar_mensaje}" id="ot3"/>                    </af:panelHeader>                </af:panelBorderLayout>            </af:dialog>        </af:popup>        <!-- nivel 4 -->        <af:popup childCreation="deferred" autoCancel="disabled" id="p4" rendered="#{attrs.nivel eq 4}">            <af:dialog id="d4" type="cancel" title="#{bndBase.msg_borrar_titulo}">                <f:facet name="buttonBar">                    <af:button id="cb4" text="#{bndBase.btn_borrar}" action="#{comp.handleBorrarAccion}">                        <af:clientListener method="hidePopupP4" type="action"/>                    </af:button>                </f:facet>                <af:panelBorderLayout id="pbl4">                    <af:panelHeader id="pgl4" text="#{bndBase.msg_borrar_advertencia}" messageType="warning">                        <af:outputText value="#{bndBase.msg_borrar_mensaje}" id="ot4"/>                    </af:panelHeader>                </af:panelBorderLayout>            </af:dialog>        </af:popup>    </af:componentDef></jsp:root>
<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:f="http://java.sun.com/jsf/core"          xmlns:af="http://xmlns.oracle.com/adf/faces/rich">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <af:componentDef var="attrs" componentVar="comp" definition="private">        <af:xmlContent>            <component xmlns="http://xmlns.oracle.com/adf/faces/rich/component">                <description/>                <display-name>botonesCancelar</display-name>                <component-class>base.botones.BotonesCancelar</component-class>                <facet>                    <description>Botones de acciones varias</description>                    <facet-name>botonesAcciones</facet-name>                </facet>                <attribute>                    <attribute-name>detalle</attribute-name>                    <attribute-class>java.lang.Boolean</attribute-class>                    <default-value>false</default-value>                </attribute>                <component-extension>                    <component-tag-namespace>base.botones</component-tag-namespace>                    <component-taglib-uri>/baseLib01</component-taglib-uri>                    <method-attribute>                        <attribute-name>inicioAccion</attribute-name>                        <method-signature>java.lang.String method()</method-signature>                    </method-attribute>                </component-extension>            </component>        </af:xmlContent>        <af:panelGroupLayout id="pgl3" layout="horizontal">            <af:facetRef facetName="botonesAcciones"/>            <af:switcher facetName="#{(attrs.detalle)?'sinListado':'conListado'}" defaultFacet="conListado" id="s2">                <f:facet name="conListado">                    <af:button text="#{bndBase.btn_cancelar}" id="b5" action="#{comp.handleInicioAccion}"                               immediate="true"                               icon="../../Imagen-001/icons/3994424_remove_delete_close_cancel_reject.svg">                        <af:resetActionListener/>                    </af:button>                </f:facet>                <f:facet name="sinListado">                    <af:button text="#{bndBase.btn_cancelar}" id="b3" immediate="true"                               icon="../../Imagen-001/icons/3994424_remove_delete_close_cancel_reject.svg">                        <af:resetActionListener/>                    </af:button>                </f:facet>            </af:switcher>            <f:facet name="separator">                <af:spacer id="s1" height="3" width="3"/>            </f:facet>        </af:panelGroupLayout>    </af:componentDef></jsp:root>
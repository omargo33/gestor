<?xml version="1.0" encoding="UTF-8" ?><adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">    <task-flow-template id="menu-base-template">        <default-activity>router1</default-activity>        <data-control-scope>            <shared/>        </data-control-scope>        <input-parameter-definition id="__1">            <name>pUsuario</name>            <value>#{pageFlowScope.pUsuario}</value>            <class>java.lang.String</class>            <required/>        </input-parameter-definition>        <input-parameter-definition id="__2">            <name>pModulo</name>            <value>#{pageFlowScope.pModulo}</value>            <class>java.lang.String</class>            <required/>        </input-parameter-definition>        <method-call id="Rollback">            <outcome>                <?audit suppress oracle.ide.xml.validation-incomplete?>                <fixed-outcome>Rollback</fixed-outcome>            </outcome>        </method-call>        <router id="router1">            <case id="__9">                <expression>#{sessionScope.idSeleccionado &gt;= 0}</expression>                <outcome>presentar</outcome>            </case>            <default-outcome>cargar</default-outcome>        </router>        <url-view id="urlView1">            <url>#{MenuBean.urlModulo}</url>        </url-view>        <method-call id="ejecutarMenusPermisos">            <method>#{bindings.ejecutarMenusPermisos.execute}</method>            <outcome>                <fixed-outcome>ejecutarMenusPermisos</fixed-outcome>            </outcome>        </method-call>        <view id="Menu">            <page>/menuEstructura/Menu.jsff</page>        </view>        <method-call id="RollbackModulo">            <outcome>                <?audit suppress oracle.ide.xml.validation-incomplete?>                <fixed-outcome>modulo</fixed-outcome>            </outcome>        </method-call>        <url-view id="UrlLogOut">            <url>../../</url>        </url-view>        <control-flow-rule id="__10">            <from-activity-id>router1</from-activity-id>            <control-flow-case id="__20">                <from-outcome>cargar</from-outcome>                <to-activity-id>ejecutarMenusPermisos</to-activity-id>            </control-flow-case>            <control-flow-case id="__30">                <from-outcome>presentar</from-outcome>                <to-activity-id>Menu</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__4">            <from-activity-id>Menu</from-activity-id>            <control-flow-case id="__5">                <from-outcome>irModulo</from-outcome>                <to-activity-id>RollbackModulo</to-activity-id>            </control-flow-case>            <control-flow-case id="__8">                <from-outcome>reload</from-outcome>                <to-activity-id>Rollback</to-activity-id>            </control-flow-case>            <control-flow-case id="__3">                <from-outcome>logout</from-outcome>                <to-activity-id>UrlLogOut</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__6">            <from-activity-id>ejecutarMenusPermisos</from-activity-id>            <control-flow-case id="__7">                <from-outcome>ejecutarMenusPermisos</from-outcome>                <to-activity-id>Menu</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__13">            <from-activity-id>Rollback</from-activity-id>            <control-flow-case id="__14">                <from-outcome>Rollback</from-outcome>                <to-activity-id>router1</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__11">            <from-activity-id>RollbackModulo</from-activity-id>            <control-flow-case id="__12">                <from-outcome>modulo</from-outcome>                <to-activity-id>urlView1</to-activity-id>            </control-flow-case>        </control-flow-rule>        <use-page-fragments/>    </task-flow-template></adfc-config>
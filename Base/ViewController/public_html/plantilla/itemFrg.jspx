<?xml version='1.0' encoding='UTF-8'?><?audit suppress oracle.jdeveloper.jsp.unreferenced-jar?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <af:pageTemplateDef var="attrs" definition="private">        <af:xmlContent>            <component xmlns="http://xmlns.oracle.com/adf/faces/rich/component">                <description>Pantalla para editar items</description>                <display-name>Item Fragmento</display-name>                <facet>                    <facet-name>elementoCentro</facet-name>                </facet>                <facet>                    <facet-name>elementoCancelar</facet-name>                </facet>                <facet>                    <facet-name>elementoAuditoria</facet-name>                </facet>                <attribute>                    <attribute-name>titulo</attribute-name>                    <attribute-class>java.lang.String</attribute-class>                </attribute>                <attribute>                    <attribute-name>tituloDescripcion</attribute-name>                    <attribute-class>java.lang.String</attribute-class>                </attribute>                <attribute>                    <attribute-name>permisoVerAuditoria</attribute-name>                    <attribute-class>java.lang.Boolean</attribute-class>                    <default-value>1</default-value>                </attribute>                            </component>        </af:xmlContent>        <af:panelGroupLayout id="pt_pgl2" layout="vertical"                             inlineStyle="background-color:White; padding:6px; flex-align:end;">            <af:panelHeader text="#{attrs.titulo}" id="pt_ph1" size="2" headerLevel="1">                <?audit suppress oracle.adf.faces.invalidTypeForHeaderComponent?>                <f:facet name="context"/>                <f:facet name="toolbar">                    <af:facetRef facetName="elementoCancelar"/>                </f:facet>                <f:facet name="legend">                    <af:outputText value="#{attrs.tituloDescripcion}" id="pt_ot1"                                   rendered="#{attrs.tituloDescripcion != null}"/>                </f:facet>                <af:showDetail disclosed="false" id="sd1" immediate="true" rendered="#{attrs.permisoVerAuditoria}">                    <af:facetRef facetName="elementoAuditoria"/>                </af:showDetail>            </af:panelHeader>            <af:panelGroupLayout id="pt_pgl1">                <af:facetRef facetName="elementoCentro"/>            </af:panelGroupLayout>            <f:facet name="separator">                <af:spacer id="pt_s4" height="10" width="10"/>            </f:facet>            <af:panelGroupLayout id="pt_pgl3"/>        </af:panelGroupLayout>    </af:pageTemplateDef></jsp:root>
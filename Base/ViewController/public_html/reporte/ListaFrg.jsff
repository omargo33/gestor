<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core">    <af:pageTemplate viewId="/plantilla/listaFrg.jspx" id="pt1">        <f:facet name="elementoBusqueda"/>        <f:facet name="elementoCentro">            <af:table value="#{bindings.Base_ArchivoView1.collectionModel}" var="row"                      rows="#{bindings.Base_ArchivoView1.rangeSize}"                      emptyText="#{bindings.Base_ArchivoView1.viewable ? 'No data to display.' : 'Access Denied.'}"                      rowBandingInterval="1" selectedRowKeys="#{bindings.Base_ArchivoView1.collectionModel.selectedRow}"                      selectionListener="#{bindings.Base_ArchivoView1.collectionModel.makeCurrent}"                      rowSelection="single" fetchSize="#{bindings.Base_ArchivoView1.rangeSize}" id="t1"                      scrollPolicy="page" columnStretching="column:c4">                <af:column headerText="#{bindings.Base_ArchivoView1.hints.IdArchivo.label}" id="c1" rendered="false">                    <af:outputText value="#{row.IdArchivo}"                                   shortDesc="#{bindings.Base_ArchivoView1.hints.IdArchivo.tooltip}" id="ot6">                        <af:convertNumber groupingUsed="false"                                          pattern="#{bindings.Base_ArchivoView1.hints.IdArchivo.format}"/>                    </af:outputText>                </af:column>                <af:column headerText="#{bindings.Base_ArchivoView1.hints.IdGrupo.label}" id="c2" rendered="false">                    <af:outputText value="#{row.IdGrupo}"                                   shortDesc="#{bindings.Base_ArchivoView1.hints.IdGrupo.tooltip}" id="ot5">                        <af:convertNumber groupingUsed="false"                                          pattern="#{bindings.Base_ArchivoView1.hints.IdGrupo.format}"/>                    </af:outputText>                </af:column>                <af:column headerText="#{bindings.Base_ArchivoView1.hints.NombreRamdon.label}" id="c3" rendered="false">                    <af:outputText value="#{row.NombreRamdon}"                                   shortDesc="#{bindings.Base_ArchivoView1.hints.NombreRamdon.tooltip}" id="ot9"/>                </af:column>                <af:column headerText="#{bindings.Base_ArchivoView1.hints.Nombre.label}" id="c4">                    <af:link text="#{row.Nombre}" id="l1"                             destination="Download?idArchivo=#{row.IdArchivo}&amp;nameUser=#{sessionScope.BaseBean.nameUser}"                             targetFrame="_blank">                    </af:link>                </af:column>                <af:column headerText="#{bindings.Base_ArchivoView1.hints.Extension.label}" id="c5">                    <af:outputText value="#{row.Extension}"                                   shortDesc="#{bindings.Base_ArchivoView1.hints.Extension.tooltip}" id="ot7"/>                </af:column>                <af:column headerText="#{bindings.Base_ArchivoView1.hints.Largo.label}" id="c6" rendered="false">                    <af:outputText value="#{row.Largo}" shortDesc="#{bindings.Base_ArchivoView1.hints.Largo.tooltip}"                                   id="ot2">                        <af:convertNumber groupingUsed="false"                                          pattern="#{bindings.Base_ArchivoView1.hints.Largo.format}"/>                    </af:outputText>                </af:column>                <af:column headerText="#{bindings.Base_ArchivoView1.hints.PathRelativo.label}" id="c7" rendered="false">                    <af:outputText value="#{row.PathRelativo}"                                   shortDesc="#{bindings.Base_ArchivoView1.hints.PathRelativo.tooltip}" id="ot4"/>                </af:column>                <af:column headerText="#{bindings.Base_ArchivoView1.hints.Estado.label}" id="c8" rendered="false">                    <af:outputText value="#{row.Estado}" shortDesc="#{bindings.Base_ArchivoView1.hints.Estado.tooltip}"                                   id="ot3"/>                </af:column>                <af:column headerText="#{bindings.Base_ArchivoView1.hints.Usuario.label}" id="c9">                    <af:outputText value="#{row.Usuario}"                                   shortDesc="#{bindings.Base_ArchivoView1.hints.Usuario.tooltip}" id="ot10"/>                </af:column>                <af:column headerText="#{bindings.Base_ArchivoView1.hints.UsuarioFecha.label}" id="c10" width="120">                    <af:outputText value="#{row.UsuarioFecha}"                                   shortDesc="#{bindings.Base_ArchivoView1.hints.UsuarioFecha.tooltip}" id="ot11">                        <af:convertDateTime pattern="#{bindings.Base_ArchivoView1.hints.UsuarioFecha.format}"/>                    </af:outputText>                </af:column>                <af:column headerText="#{bindings.Base_ArchivoView1.hints.UsuarioPrograma.label}" id="c11"                           rendered="false">                    <af:outputText value="#{row.UsuarioPrograma}"                                   shortDesc="#{bindings.Base_ArchivoView1.hints.UsuarioPrograma.tooltip}" id="ot8"/>                </af:column>            </af:table>        </f:facet>        <f:facet name="elementoAcciones"/>        <f:attribute name="titulo" value="#{bndBase.reporte_listaFrg_titulo}"/>        <f:attribute name="tituloDescripcion" value="#{bndBase.reporte_listaFrg_descripcion}"/>    </af:pageTemplate></jsp:root>
<?xml version="1.0" encoding="UTF-8" ?><adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">  <task-flow-definition id="info-usuario-task-flow">    <default-activity>base_rolesByNick</default-activity>    <data-control-scope>      <shared/>    </data-control-scope>    <input-parameter-definition id="__5">      <name>nick</name>      <value>#{pageFlowScope.nick}</value>      <class>java.lang.String</class>    </input-parameter-definition>    <view id="usuario">      <page>/infoUsuario/usuario.jsff</page>    </view>    <method-call id="base_rolesByNick">      <method>#{bindings.base_rolesByNick.execute}</method>      <return-value>#{sessionScope.rolesActivos}</return-value>      <outcome>        <fixed-outcome>base_rolesByNick</fixed-outcome>      </outcome>    </method-call>    <method-call id="ejecutarByNick">      <method>#{bindings.ejecutarByNick.execute}</method>      <outcome>        <fixed-outcome>ejecutarByNick</fixed-outcome>      </outcome>    </method-call>    <control-flow-rule id="__1">      <from-activity-id>base_rolesByNick</from-activity-id>      <control-flow-case id="__2">        <from-outcome>base_rolesByNick</from-outcome>        <to-activity-id>ejecutarByNick</to-activity-id>      </control-flow-case>    </control-flow-rule>    <control-flow-rule id="__3">      <from-activity-id>ejecutarByNick</from-activity-id>      <control-flow-case id="__4">        <from-outcome>ejecutarByNick</from-outcome>        <to-activity-id>usuario</to-activity-id>      </control-flow-case>    </control-flow-rule>    <use-page-fragments/>  </task-flow-definition></adfc-config>
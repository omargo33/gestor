<?xml version="1.0" encoding="UTF-8" ?><adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">  <task-flow-definition id="inicio-login-task-flow">    <default-activity>irInicio</default-activity>    <data-control-scope>      <shared/>    </data-control-scope>    <method-call id="base_obtenerParametroTexto01">      <method>#{bindings.base_obtenerParametroTexto01.execute}</method>      <return-value>#{sessionScope.urlLogout}</return-value>      <outcome>        <fixed-outcome>base_obtenerParametroTexto01</fixed-outcome>      </outcome>    </method-call>    <view id="irInicio">      <page>/error/irInicio.jsff</page>    </view>    <url-view id="urlView1">      <url>#{sessionScope.urlLogout}</url>    </url-view>    <control-flow-rule id="__1">      <from-activity-id>base_obtenerParametroTexto01</from-activity-id>      <control-flow-case id="__2">        <from-outcome>base_obtenerParametroTexto01</from-outcome>        <to-activity-id>urlView1</to-activity-id>      </control-flow-case>    </control-flow-rule>    <control-flow-rule id="__3">      <from-activity-id>irInicio</from-activity-id>      <control-flow-case id="__4">        <from-outcome>salir</from-outcome>        <to-activity-id>base_obtenerParametroTexto01</to-activity-id>      </control-flow-case>    </control-flow-rule>    <use-page-fragments/>  </task-flow-definition></adfc-config>
<?xml version='1.0' encoding='UTF-8'?><?audit suppress oracle.jdeveloper.jsp.unreferenced-jar?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core">    <af:panelGroupLayout id="pgl1">        <af:pageTemplate viewId="/plantilla/listaFrg.jspx" id="pt1">            <f:attribute name="titulo" value="#{bndBase.archivo_listaFrg_titulo}"/>            <f:attribute name="tituloDescripcion" value="#{bndBase.archivo_listaFrg_descripcion}"/>            <f:facet name="elementoBusqueda"/>            <f:facet name="elementoCentro">                <af:table value="#{bindings.Base_ArchivoView2.collectionModel}" var="row"                          rows="#{bindings.Base_ArchivoView2.rangeSize}"                          emptyText="#{bindings.Base_ArchivoView2.viewable ? 'No data to display.' : 'Access Denied.'}"                          rowBandingInterval="1"                          selectedRowKeys="#{bindings.Base_ArchivoView2.collectionModel.selectedRow}"                          selectionListener="#{bindings.Base_ArchivoView2.collectionModel.makeCurrent}"                          rowSelection="single" fetchSize="#{bindings.Base_ArchivoView2.rangeSize}" id="t1"                          columnStretching="column:c4" scrollPolicy="page"                          binding="#{backingBeanScope.PPRBackingArchivoListFrg.t1}">                    <af:column headerText="#{bindings.Base_ArchivoView2.hints.IdArchivo.label}" id="c1" visible="false">                        <af:outputText value="#{row.IdArchivo}"                                       shortDesc="#{bindings.Base_ArchivoView2.hints.IdArchivo.tooltip}" id="ot1">                            <af:convertNumber groupingUsed="false"                                              pattern="#{bindings.Base_ArchivoView2.hints.IdArchivo.format}"/>                        </af:outputText>                    </af:column>                    <af:column headerText="#{bindings.Base_ArchivoView2.hints.IdGrupo.label}" id="c2" visible="false">                        <af:outputText value="#{row.IdGrupo}"                                       shortDesc="#{bindings.Base_ArchivoView2.hints.IdGrupo.tooltip}" id="ot2">                            <af:convertNumber groupingUsed="false"                                              pattern="#{bindings.Base_ArchivoView2.hints.IdGrupo.format}"/>                        </af:outputText>                    </af:column>                    <af:column headerText="#{bindings.Base_ArchivoView2.hints.NombreRamdon.label}" id="c3"                               visible="false">                        <af:outputText value="#{row.NombreRamdon}"                                       shortDesc="#{bindings.Base_ArchivoView2.hints.NombreRamdon.tooltip}" id="ot3"/>                    </af:column>                    <af:column headerText="#{bindings.Base_ArchivoView2.hints.Nombre.label}" id="c4">                        <af:link text="#{row.Nombre}" id="l1" action="Editar"/>                    </af:column>                    <af:column headerText="#{bindings.Base_ArchivoView2.hints.Extension.label}" id="c5">                        <af:outputText value="#{row.Extension}"                                       shortDesc="#{bindings.Base_ArchivoView2.hints.Extension.tooltip}" id="ot5"/>                    </af:column>                    <af:column headerText="#{bindings.Base_ArchivoView2.hints.Largo.label}" id="c6" visible="false">                        <af:outputText value="#{row.Largo}"                                       shortDesc="#{bindings.Base_ArchivoView2.hints.Largo.tooltip}" id="ot6">                            <af:convertNumber groupingUsed="false"                                              pattern="#{bindings.Base_ArchivoView2.hints.Largo.format}"/>                        </af:outputText>                    </af:column>                    <af:column headerText="#{bindings.Base_ArchivoView2.hints.PathRelativo.label}" id="c7"                               visible="false">                        <af:outputText value="#{row.PathRelativo}"                                       shortDesc="#{bindings.Base_ArchivoView2.hints.PathRelativo.tooltip}" id="ot7"/>                    </af:column>                    <af:column headerText="#{bindings.Base_ArchivoView2.hints.Estado.label}" id="c8" visible="false">                        <af:outputText value="#{row.Estado}"                                       shortDesc="#{bindings.Base_ArchivoView2.hints.Estado.tooltip}" id="ot8"/>                    </af:column>                    <af:column headerText="#{bindings.Base_ArchivoView2.hints.Usuario.label}" id="c9">                        <af:outputText value="#{row.Usuario}"                                       shortDesc="#{bindings.Base_ArchivoView2.hints.Usuario.tooltip}" id="ot9"/>                    </af:column>                    <af:column headerText="#{bindings.Base_ArchivoView2.hints.UsuarioFecha.label}" id="c10" width="120">                        <af:outputText value="#{row.UsuarioFecha}"                                       shortDesc="#{bindings.Base_ArchivoView2.hints.UsuarioFecha.tooltip}" id="ot10">                            <af:convertDateTime pattern="#{bindings.Base_ArchivoView2.hints.UsuarioFecha.format}"/>                        </af:outputText>                    </af:column>                    <af:column headerText="#{bindings.Base_ArchivoView2.hints.UsuarioPrograma.label}" id="c11"                               visible="false">                        <af:outputText value="#{row.UsuarioPrograma}"                                       shortDesc="#{bindings.Base_ArchivoView2.hints.UsuarioPrograma.tooltip}"                                       id="ot11"/>                    </af:column>                </af:table>            </f:facet>            <f:facet name="elementoAcciones">                <af:button text="#{bndBase.archivo_listaFrg_boton_crear}" id="b1"                           icon="../../Imagen-001/icons/3994437_create_positive_add_plus_new.svg"                           rendered="#{pageFlowScope.pPermisoCrear}">                    <af:showPopupBehavior popupId="::p1Arch"/>                </af:button>            </f:facet>        </af:pageTemplate>        <af:popup childCreation="deferred" autoCancel="disabled" id="p1Arch"                  binding="#{backingBeanScope.PPRBackingArchivoListFrg.p1Arch}">            <af:dialog id="d1" type="none" title="#{bndBase.archivo_listaFrg_boton_crear}">                <f:facet name="buttonBar">                    <af:button id="cb3" text="#{bndBase.btn_cancelar}"                               action="#{backingBeanScope.PPRBackingArchivoListFrg.refrescar}"/>                </f:facet>                <af:inlineFrame id="if1" shortDesc="#{bndBase.btn_cancelar}"                                binding="#{backingBeanScope.PPRBackingArchivoListFrg.if1}" inlineStyle="width:45vw;"/>            </af:dialog>        </af:popup>    </af:panelGroupLayout></jsp:root>
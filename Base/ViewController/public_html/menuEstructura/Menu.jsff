<?xml version='1.0' encoding='UTF-8'?><?audit suppress oracle.jdeveloper.jsp.unreferenced-jar?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:f="http://java.sun.com/jsf/core"          xmlns:af="http://xmlns.oracle.com/adf/faces/rich" xmlns:c="http://java.sun.com/jsp/jstl/core">    <c:set var="viewcontrollerBundle" value="#{adfBundle['view.ViewControllerBundle']}"/>    <af:panelGroupLayout id="pgl1" inlineStyle="border-top-width:70px; " styleClass="panelDarkBlue" layout="vertical">        <af:panelGroupLayout id="pgl10" halign="center" layout="vertical" valign="bottom"                             inlineStyle="padding-top:23px; padding-bottom:23px; padding-left:7px; padding-right:7px;">            <af:image source="../../Imagen-001/icons/3994415_user_account_profile_person_avatar.svg"                      styleClass="estilo32 estiloBlanco" id="i1" shortDesc="person_avatar"/>            <af:outputText value="#{pageFlowScope.pUsuario}" id="of1"                           inlineStyle="font-weight:bolder; text-transform: uppercase;" styleClass="colorBlanco"                           truncateAt="24"/>            <f:facet name="separator">                <af:spacer id="s1" height="3" width="3"/>            </f:facet>        </af:panelGroupLayout>        <af:outputText value="#{MenuBean.tituloModulo} " id="ot1"                       inlineStyle="font-weight:bolder; font-size:large; text-align:center; padding-left: 5px;"                       styleClass="colorRojo"/>        <af:panelGroupLayout id="pgl20" layout="scroll"                             inlineStyle="height:#{(MenuBean.menuSize gt 6 ? '259': (MenuBean.menuSize*37))}px; width: 160px;"                             styleClass="menuScroll" halign="left">            <af:forEach var="vir" items="#{MenuBean.listaMenu}">                <af:panelGroupLayout id="pgl4" layout="horizontal" valign="middle" styleClass="menu">                    <af:link id="l4" action="#{vir.ejecutarMenu}" disabled="#{vir.seleccionado}" immediate="true">                        <af:outputText value="#{vir.nombreMenu}" id="ot3"/>                    </af:link>                    <af:image source="../../Imagen-001/icons/3994400_forward_navigation_right_next_arrow.svg"                              shortDesc="1" rendered="#{vir.seleccionado}" id="i2" styleClass="estilo16 estiloBlanco"/>                </af:panelGroupLayout>            </af:forEach>        </af:panelGroupLayout>        <af:spacer id="s3" height="5" width="5" rendered="#{MenuBean.moduloSize gt 0 }"/>        <af:outputText value="#{viewcontrollerBundle.menu_modulos}" id="ot2"                       inlineStyle="font-weight:bolder; font-size:large; text-align:center; padding-left: 5px;"                       styleClass="colorRojo" rendered="#{MenuBean.moduloSize gt 0 }"/>        <af:panelGroupLayout id="pgl2" layout="scroll"                             inlineStyle="height:#{(MenuBean.moduloSize gt 3 ? '148': (MenuBean.moduloSize*37))}px; width: 160px;"                             styleClass="menuScroll" halign="left" rendered="#{MenuBean.moduloSize gt 0 }">            <af:forEach var="vir" items="#{MenuBean.listaModulo}">                <af:panelGroupLayout id="pgl5" layout="horizontal" valign="middle" styleClass="menu">                    <af:link text="#{vir.nombreModulo}" id="l8" immediate="true" action="#{vir.ejecutarModulo}"                             iconPosition="trailing"/>                </af:panelGroupLayout>            </af:forEach>        </af:panelGroupLayout>        <af:spacer id="s4" height="5" width="5"/>        <af:outputText value="Sesion" id="ot4"                       inlineStyle="font-weight:bolder; font-size:large; text-align:center; padding-left: 5px;"                       styleClass="colorRojo"/>        <af:panelGroupLayout id="pgl6" inlineStyle="height:37px;  width: 160px;" styleClass="menuScroll"                             halign="left">            <af:panelGroupLayout id="pgl7" layout="horizontal" valign="middle" styleClass="menu">                <af:image source="../../Imagen-001/icons/3994382_close_access_sign out_exit_logout.svg" shortDesc="1"                          id="i3" styleClass="estilo16 estiloBlanco"/>                <af:link text="#{bndBase.menu_cerrar_sesion}" id="l1" immediate="true" action="logout"/>            </af:panelGroupLayout>        </af:panelGroupLayout>        <af:panelGroupLayout id="pgl3" layout="vertical" valign="top" visible="true" inlineStyle="height:60vh;"/>    </af:panelGroupLayout>    <!--oracle-jdev-comment:preferred-managed-bean-name:managedBean1--></jsp:root>
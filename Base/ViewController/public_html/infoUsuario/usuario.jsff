<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core">  <af:panelHeader text="#{bndBase.info_usuario_titulo}" id="ph1" messageType="info" size="3" headerLevel="3">    <f:facet name="context"/>    <f:facet name="toolbar"/>    <f:facet name="menuBar"/>    <f:facet name="legend"/>    <f:facet name="info"/>    <af:panelFormLayout id="pfl1">      <af:inputText value="#{bindings.Nick.inputValue}" label="#{bindings.Nick.hints.label}"                    required="#{bindings.Nick.hints.mandatory}" columns="#{bindings.Nick.hints.displayWidth}"                    maximumLength="#{bindings.Nick.hints.precision}" shortDesc="#{bindings.Nick.hints.tooltip}"                    id="it2">        <f:validator binding="#{bindings.Nick.validator}"/>      </af:inputText>      <af:inputText value="#{bindings.Nombre.inputValue}" label="#{bindings.Nombre.hints.label}"                    required="#{bindings.Nombre.hints.mandatory}" columns="#{bindings.Nombre.hints.displayWidth}"                    maximumLength="#{bindings.Nombre.hints.precision}" shortDesc="#{bindings.Nombre.hints.tooltip}"                    id="it3">        <f:validator binding="#{bindings.Nombre.validator}"/>      </af:inputText>      <af:inputText value="#{bindings.Apellido.inputValue}" label="#{bindings.Apellido.hints.label}"                    required="#{bindings.Apellido.hints.mandatory}" columns="#{bindings.Apellido.hints.displayWidth}"                    maximumLength="#{bindings.Apellido.hints.precision}" shortDesc="#{bindings.Apellido.hints.tooltip}"                    id="it4">        <f:validator binding="#{bindings.Apellido.validator}"/>      </af:inputText>      <af:inputText value="#{sessionScope.rolesActivos}" label="Roles" columns="#{bindings.Apellido.hints.displayWidth}"                    id="it5" rows="3"/>    </af:panelFormLayout>  </af:panelHeader></jsp:root>
<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:f="http://java.sun.com/jsf/core"          xmlns:af="http://xmlns.oracle.com/adf/faces/rich">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <f:view>        <af:document title="upLoadFile" id="d1">            <af:panelGroupLayout id="pgl4">                <af:panelGroupLayout id="pgl2" layout="vertical">                    <af:panelGroupLayout id="pgl1" layout="vertical" halign="center">                        <f:verbatim escape="false" rendered="true">                        <form method="POST" action="Upload" enctype="multipart/form-data">                            <br/>                            <input type="file" name="file" size="40"/>                            <br/>                            <br/>                            <input type="submit" value="Cargar"/>                            <br/>                            <input name="aExtensiones" size="40" value="" id="aExtensiones" type="hidden"/>                            <input name="aEsquema" size="40" value="" id="aEsquema" type="hidden"/>                            <input name="aTabla" size="40" value="" id="aTabla" type="hidden"/>                            <input name="idGrupo" size="40" value="" id="idGrupo" type="hidden"/>                            <input name="nameUser" size="40" value="" id="nameUser" type="hidden"/>                            <script>                              var url_string = window.location.href;                              var url = new URL(url_string);                              var pAExtensiones = url.searchParams.get("aExtensiones");                              document.getElementById("aExtensiones").value = pAExtensiones;                              var pAEsquema = url.searchParams.get("aEsquema");                              document.getElementById("aEsquema").value = pAEsquema;                              var pATabla = url.searchParams.get("aTabla");                              document.getElementById("aTabla").value = pATabla;                              var pIdGrupo = url.searchParams.get("idGrupo");                              document.getElementById("idGrupo").value = pIdGrupo;                              var pNameUser = url.searchParams.get("nameUser");                              document.getElementById("nameUser").value = pNameUser;                            </script>                        </form>                        </f:verbatim>                    </af:panelGroupLayout>                    <af:panelHeader text="Informaci&#243;n Carga" id="ph1"                                    messageType="#{(param.tipo ne null)? ( param.tipo): 'none'}" size="2"                                    headerLevel="2" type="default">                        <?audit suppress oracle.adf.faces.invalidTypeForHeaderComponent?>                        <f:facet name="context"/>                        <f:facet name="toolbar"/>                        <f:facet name="menuBar"/>                        <f:facet name="legend">                            <af:outputText value="#{(param.mensaje ne null)? (param.mensaje) : 'Seleccione un archivo para subir al aplicativo'}"                                           id="ot1"/>                        </f:facet>                        <f:facet name="info"/>                    </af:panelHeader>                </af:panelGroupLayout>                <af:panelGroupLayout id="pgl3"></af:panelGroupLayout>            </af:panelGroupLayout>        </af:document>    </f:view></jsp:root>
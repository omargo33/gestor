"/>                    <f:facet name="toolbar"/>                    <f:facet name="menuBar"/>                    <f:facet name="legend"/>                    <f:facet name="info"/>                    <af:outputText value="#{bndBase.presentacion_parafo_2}" id="ot3"/>                </af:panelHeader>                <af:panelHeader text="#{bndBase.presentacion_encabezado_3}" id="ph3" size="2" headerLevel="2">                    <f:facet name="context"/>                    <f:facet name="toolbar"/>                    <f:facet name="menuBar"/>                    <f:facet name="legend"/>                    <f:facet name="info"/>                    <af:outputText value="#{bndBase.presentacion_parafo_3}" id="ot4"/>                </af:panelHeader>            </af:panelHeader>        </af:panelGroupLayout>                <f:facet name="separator"/>    </af:panelGroupLayout></jsp:root>
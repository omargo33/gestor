<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core" xmlns:bl01="/baseLib01">    <af:panelGroupLayout id="pgl4">        <af:pageTemplate viewId="/plantilla/itemFrg.jspx" value="#{bindings.ptb1}" id="pt1">            <f:attribute name="titulo" value="#{bnd.menu_permiso_itemFrg_titulo}"/>            <f:attribute name="tituloDescripcion" value="#{bnd.menu_permiso_itemFrg_descripcion}"/>            <f:attribute name="permisoVerAuditoria" value="#{pageFlowScope.hPermisoVerAuditoria}"/>            <f:facet name="elementoCentro">                <af:panelFormLayout id="pfl1" labelWidth="140">                    <af:selectOneChoice value="#{bindings.IdRol.inputValue}" label="#{bindings.IdRol.label}"                                        required="#{bindings.IdRol.hints.mandatory}"                                        shortDesc="#{bindings.IdRol.hints.tooltip}" id="soc2">                        <f:selectItems value="#{bindings.IdRol.items}" id="si2"/>                        <f:validator binding="#{bindings.IdRol.validator}"/>                    </af:selectOneChoice>                    <af:selectOneChoice value="#{bindings.Crear.inputValue}" label="#{bindings.Crear.label}"                                        required="#{bindings.Crear.hints.mandatory}"                                        shortDesc="#{bindings.Crear.hints.tooltip}" id="soc3">                        <f:selectItems value="#{bindings.Crear.items}" id="si3"/>                        <f:validator binding="#{bindings.Crear.validator}"/>                    </af:selectOneChoice>                    <af:selectOneChoice value="#{bindings.Actualizar.inputValue}" label="#{bindings.Actualizar.label}"                                        required="#{bindings.Actualizar.hints.mandatory}"                                        shortDesc="#{bindings.Actualizar.hints.tooltip}" id="soc4">                        <f:selectItems value="#{bindings.Actualizar.items}" id="si4"/>                        <f:validator binding="#{bindings.Actualizar.validator}"/>                    </af:selectOneChoice>                    <af:selectOneChoice value="#{bindings.Borrar.inputValue}" label="#{bindings.Borrar.label}"                                        required="#{bindings.Borrar.hints.mandatory}"                                        shortDesc="#{bindings.Borrar.hints.tooltip}" id="soc5">                        <f:selectItems value="#{bindings.Borrar.items}" id="si5"/>                        <f:validator binding="#{bindings.Borrar.validator}"/>                    </af:selectOneChoice>                    <af:selectOneChoice value="#{bindings.VerAuditoria.inputValue}"                                        label="#{bindings.VerAuditoria.label}"                                        required="#{bindings.VerAuditoria.hints.mandatory}"                                        shortDesc="#{bindings.VerAuditoria.hints.tooltip}" id="soc6">                        <f:selectItems value="#{bindings.VerAuditoria.items}" id="si6"/>                        <f:validator binding="#{bindings.VerAuditoria.validator}"/>                    </af:selectOneChoice>                    <f:facet name="footer">                        <bl01:botonesGuardarBorrar id="bgb1" nivel="2"                                                   actualizarAccion="#{BackingItemFrgHijo.actionGuardar}"                                                   actualizarPermiso="#{pageFlowScope.hPermisoActualizar}"                                                   borrarAccion="#{BackingItemFrgHijo.actionDelete}"                                                   borrarDisabled="#{bindings.IdPermiso.inputValue eq 0}"                                                   borrarPermiso="#{pageFlowScope.hPermisoBorrar}"/>                    </f:facet>                </af:panelFormLayout>            </f:facet>            <f:facet name="elementoCancelar">                <bl01:botonesCancelar id="bc1" inicioAccion="Inicio">                    <f:facet name="botonesAcciones"/>                </bl01:botonesCancelar>            </f:facet>            <f:facet name="elementoAuditoria">                <af:panelFormLayout id="pfl2" labelWidth="110">                    <f:facet name="footer"/>                    <af:inputText value="#{bindings.IdPermiso.inputValue}" label="#{bindings.IdPermiso.hints.label}"                                  required="#{bindings.IdPermiso.hints.mandatory}"                                  columns="#{bindings.IdPermiso.hints.displayWidth}"                                  maximumLength="#{bindings.IdPermiso.hints.precision}"                                  shortDesc="#{bindings.IdPermiso.hints.tooltip}" id="it1">                        <f:validator binding="#{bindings.IdPermiso.validator}"/>                        <af:convertNumber groupingUsed="false" pattern="#{bindings.IdPermiso.format}"/>                    </af:inputText>                    <af:selectOneChoice value="#{bindings.IdMenu.inputValue}" label="#{bindings.IdMenu.label}"                                        required="#{bindings.IdMenu.hints.mandatory}"                                        shortDesc="#{bindings.IdMenu.hints.tooltip}" id="soc1">                        <f:selectItems value="#{bindings.IdMenu.items}" id="si1"/>                        <f:validator binding="#{bindings.IdMenu.validator}"/>                    </af:selectOneChoice>                    <af:inputText value="#{bindings.Usuario.inputValue}" label="#{bindings.Usuario.hints.label}"                                  required="#{bindings.Usuario.hints.mandatory}"                                  columns="#{bindings.Usuario.hints.displayWidth}"                                  maximumLength="#{bindings.Usuario.hints.precision}"                     
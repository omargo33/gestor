<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core" xmlns:bl01="/baseLib01">    <af:panelGroupLayout id="pgl1">        <af:pageTemplate viewId="/plantilla/listaFrg.jspx" value="#{bindings.ptb1}" id="pt1">            <f:attribute name="titulo" value="#{bnd.menu_listaFrg_titulo}"/>            <f:attribute name="tituloDescripcion" value="#{bnd.menu_listaFrg_descripcion}"/>            <f:facet name="elementoBusqueda">                <af:query id="qryId1" value="#{bindings.MenuViewCriteriaQuery.queryDescriptor}"                          model="#{bindings.MenuViewCriteriaQuery.queryModel}"                          queryListener="#{bindings.MenuViewCriteriaQuery.processQuery}"                          queryOperationListener="#{bindings.MenuViewCriteriaQuery.processQueryOperation}"                          headerText="#{bnd.msg_busquea_titulo}" conjunctionReadOnly="true"                          searchButtonText="#{bnd.btn_buscar}" resetButtonText="#{bnd.btn_limpiar}" labelWidth="240"                          saveQueryMode="hidden" modeChangeVisible="false" disclosed="true"                          resultComponentId="::resId1"/>            </f:facet>            <f:facet name="elementoCentro">                <af:table value="#{bindings.MenuView1.collectionModel}" var="row" rows="#{bindings.MenuView1.rangeSize}"                          emptyText="#{bindings.MenuView1.viewable ? 'No data to display.' : 'Access Denied.'}"                          rowBandingInterval="0" selectedRowKeys="#{bindings.MenuView1.collectionModel.selectedRow}"                          selectionListener="#{bindings.MenuView1.collectionModel.makeCurrent}" rowSelection="single"                          fetchSize="#{bindings.MenuView1.rangeSize}" id="resId1" columnStretching="column:resId1c5"                          scrollPolicy="page">                    <?audit suppress oracle.adf.faces.compreqsummary?><?audit suppress oracle.adf.faces.compreqsummary?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?>                    <af:column headerText="#{bindings.MenuView1.hints.IdMenu.label}" id="resId1c1">                        <af:outputText value="#{row.IdMenu}" shortDesc="#{bindings.MenuView1.hints.IdMenu.tooltip}"                                       id="ot1">                            <af:convertNumber groupingUsed="false" pattern="#{bindings.MenuView1.hints.IdMenu.format}"/>                        </af:outputText>                    </af:column>                    <af:column headerText="#{bindings.MenuView1.hints.Indice.label}" id="resId1c4">                        <af:link id="l1" action="Editar">                            <af:outputText value="#{row.Indice}" shortDesc="#{bindings.MenuView1.hints.Indice.tooltip}"                                           id="ot2"/>                        </af:link>                    </af:column>                    <af:column headerText="#{bindings.MenuView1.hints.Nombre.label}" id="resId1c5">                        <af:outputText value="#{row.Nombre}" shortDesc="#{bindings.MenuView1.hints.Nombre.tooltip}"                                       id="ot3"/>                    </af:column>                    <af:column headerText="#{bindings.MenuView1.hints.IdModulo.label}" id="resId1c2">                        <af:selectOneChoice value="#{row.bindings.IdModulo.inputValue}"                                            label="#{row.bindings.IdModulo.label}"                                            required="#{bindings.MenuView1.hints.IdModulo.mandatory}"                                            shortDesc="#{bindings.MenuView1.hints.IdModulo.tooltip}" readOnly="true"      
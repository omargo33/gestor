<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core" xmlns:bl01="/baseLib01">    <af:panelGroupLayout id="pgl4">        <af:pageTemplate viewId="/plantilla/itemFrg.jspx" id="pt1">            <f:attribute name="titulo" value="#{bnd.error_itemFrg_titulo}"/>            <f:attribute name="tituloDescripcion" value="#{bnd.error_itemFrg_descripcion}"/>            <f:attribute name="permisoVerAuditoria" value="#{pageFlowScope.pPermisoVerAuditoria}"/>            <f:facet name="elementoCentro">                <af:panelFormLayout id="pfl1" labelWidth="140">                    <af:inputText value="#{bindings.Indice.inputValue}" label="#{bindings.Indice.hints.label}"                                  required="#{bindings.Indice.hints.mandatory}"                                  columns="#{bindings.Indice.hints.displayWidth}"                                  maximumLength="#{bindings.Indice.hints.precision}"                                  shortDesc="#{bindings.Indice.hints.tooltip}" id="it2">                        <f:validator binding="#{bindings.Indice.validator}"/>                    </af:inputText>                    <af:inputText value="#{bindings.Mensaje.inputValue}" label="#{bindings.Mensaje.hints.label}"                                  required="#{bindings.Mensaje.hints.mandatory}"                                  columns="#{bindings.Mensaje.hints.displayWidth}"                                  maximumLength="#{bindings.Mensaje.hints.precision}"                                  shortDesc="#{bindings.Mensaje.hints.tooltip}" id="it3"                                  rows="#{bindings.Mensaje.hints.displayHeight}">                        <f:validator binding="#{bindings.Mensaje.validator}"/>                    </af:inputText>                    <af:inputText value="#{bindings.Descripcion.inputValue}" label="#{bindings.Descripcion.hints.label}"                                  required="#{bindings.Descripcion.hints.mandatory}"                                  columns="#{bindings.Descripcion.hints.displayWidth}"                                  maximumLength="#{bindings.Descripcion.hints.precision}"                                  shortDesc="#{bindings.Descripcion.hints.tooltip}" id="it4"                                  rows="#{bindings.Descripcion.hints.displayHeight}">                        <f:validator binding="#{bindings.Descripcion.validator}"/>                    </af:inputText>                    <f:facet name="footer">                        <bl01:botonesGuardarBorrar id="bgb1" actualizarAccion="#{BackingItemFrg.actionGuardar}"                                                   actualizarPermiso="#{pageFlowScope.pPermisoActualizar}"                                                   borrarAccion="#{BackingItemFrg.actionDelete}"                                                   borrarDisabled="#{bindings.IdError.inputValue eq 0}"                                                   borrarPermiso="#{pageFlowScope.pPermisoBorrar}"/>                    </f:facet>                </af:panelFormLayout>            </f:facet>            <f:facet name="elementoCancelar">                <bl01:botonesCancelar id="bc1" inicioAccion="Inicio">                    <f:facet name="botonesAcciones"/>                </bl01:botonesCancelar>            </f:facet>            <f:facet name="elementoAuditoria">                <af:panelFormLayout id="pfl2" labelWidth="110">                    <f:facet name="footer"/>                    <af:inputText value="#{bindings.IdError.inputValue}" label="#{bindings.IdError.hints.label}"                                  required="#{bindings.IdError.hints.mandatory}"                                  columns="#{bindings.IdError.hints.displayWidth}"                                  maximumLength="#{bindings.IdError.hints.precision}"                                  shortDesc="#{bindings.IdError.hints.tooltip}" id="it1">                        <f:validator binding="#{bindings.IdError.validator}"/>                        <af:convertNumber groupingUsed="false" pattern="#{bindings.IdError.format}"/>                    </af:inputText>                    <af:inputText value="#{bindings.Usuario.inputValue}" label="#{bindings.Usuario.hints.label}"                                  required="#{bindings.Usuario.hints.mandatory}"                                  columns="#{bindings.Usuario.hints.displayWidth}"                                  maximumLength="#{bindings.Usuario.hints.precision}"                                  shortDesc="#{bindings.Usuario.hints.tooltip}" id="it5">                        <f:validator binding="#{bindings.Usuario.validator}"/>                    </af:inputText>                    <af:inputDate value="#{bindings.UsuarioFecha.inputValue}"                                  label="#{bindings.UsuarioFecha.hints.label}"                                  required="#{bindings.UsuarioFecha.hints.mandatory}"                                  columns="#{bindings.UsuarioFecha.hints.displayWidth}"                                  shortDesc="#{bindings.UsuarioFecha.hints.tooltip}" id="id1">                        <f:validator binding="#{bindings.UsuarioFecha.validator}"/>                        <af:convertDateTime pattern="#{bindings.UsuarioFecha.format}"/>                    </af:inputDate>                    <af:inputText value="#{bindings.UsuarioPrograma.inputValue}"                                  label="#{bindings.UsuarioPrograma.hints.label}"                                  required="#{bindings.UsuarioPrograma.hints.mandatory}"                                  columns="#{bindings.UsuarioPrograma.hints.displayWidth}"                                  maximumLength="#{bindings.UsuarioPrograma.hints.precision}"                                  shortDesc="#{bindings.UsuarioPrograma.hints.tooltip}" id="it6">                        <f:validator binding="#{bindings.UsuarioPrograma.validator}"/>                    </af:inputText>                </af:panelFormLayout>            </f:facet>        </af:pageTemplate>    </af:panelGroupLayout></jsp:root>
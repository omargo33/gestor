<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core" xmlns:bl01="/baseLib01">    <af:pageTemplate viewId="/plantilla/listaFrg.jspx" id="pt1" value="#{bindings.ptb1}">        <f:attribute name="titulo" value="#{bnd.error_listaFrg_titulo}"/>        <f:attribute name="tituloDescripcion" value="#{bnd.error_listaFrg_descripcion}"/>        <f:facet name="elementoBusqueda">            <af:query id="qryId1" value="#{bindings.ErrorViewCriteriaQuery.queryDescriptor}"                      model="#{bindings.ErrorViewCriteriaQuery.queryModel}"                      queryListener="#{bindings.ErrorViewCriteriaQuery.processQuery}"                      queryOperationListener="#{bindings.ErrorViewCriteriaQuery.processQueryOperation}"                      headerText="#{bnd.msg_busquea_titulo}" conjunctionReadOnly="true"                      searchButtonText="#{bnd.btn_buscar}" resetButtonText="#{bnd.btn_limpiar}" labelWidth="240"                      saveQueryMode="hidden" modeChangeVisible="false" disclosed="true" resultComponentId="::resId1"/>        </f:facet>        <f:facet name="elementoCentro">            <af:table value="#{bindings.ErrorView1.collectionModel}" var="row" rows="#{bindings.ErrorView1.rangeSize}"                      emptyText="#{bindings.ErrorView1.viewable ? 'No data to display.' : 'Access Denied.'}"                      rowBandingInterval="0" selectedRowKeys="#{bindings.ErrorView1.collectionModel.selectedRow}"                      selectionListener="#{bindings.ErrorView1.collectionModel.makeCurrent}" rowSelection="single"                      fetchSize="#{bindings.ErrorView1.rangeSize}" id="resId1" columnStretching="column:resId1c4"                      scrollPolicy="page">                <?audit suppress oracle.adf.faces.compreqsummary?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?>                <af:column headerText="#{bindings.ErrorView1.hints.IdError.label}" id="resId1c1">                    <af:outputText value="#{row.IdError}" shortDesc="#{bindings.ErrorView1.hints.IdError.tooltip}"                                   id="ot1">                        <af:convertNumber groupingUsed="false" pattern="#{bindings.ErrorView1.hints.IdError.format}"/>                    </af:outputText>                </af:column>                <af:column headerText="#{bindings.ErrorView1.hints.Indice.label}" id="resId1c2">                    <af:link id="l1" action="Editar">                        <af:outputText value="#{row.Indice}" shortDesc="#{bindings.ErrorView1.hints.Indice.tooltip}"                                       id="ot2"/>                    </af:link>                </af:column>                <af:column headerText="#{bindings.ErrorView1.hints.Mensaje.label}" id="resId1c3">                    <af:outputText value="#{row.Mensaje}" shortDesc="#{bindings.ErrorView1.hints.Mensaje.tooltip}"                                   id="ot3"/>                </af:column>                <af:column headerText="#{bindings.ErrorView1.hints.Descripcion.label}" id="resId1c4">                    <af:outputText value="#{row.Descripcion}"                                   shortDesc="#{bindings.ErrorView1.hints.Descripcion.tooltip}" id="ot4"/>                </af:column>            </af:table>        </f:facet>        <f:facet name="elementoAcciones">            <bl01:botonesAtrasCrear id="bac1" atrasAccion="Inicio" crearAccion="Create"                                    crearTexto="#{bnd.error_listaFrg_boton_crear}"                                    crearPermiso="#{pageFlowScope.pPermisoCrear}"/>        </f:facet>    </af:pageTemplate></jsp:root>
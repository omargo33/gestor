<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core" xmlns:bl01="/baseLib01">    <af:panelGroupLayout id="pgl2">        <af:pageTemplate viewId="/plantilla/listaFrg.jspx"  id="pt1">            <f:attribute name="titulo" value="#{bnd.usuario_token_listaFrg_titulo}"/>            <f:attribute name="tituloDescripcion" value="#{bnd.usuario_token_listaFrg_descripcion}"/>            <f:facet name="elementoBusqueda"/>            <f:facet name="elementoCentro">                <af:switcher facetName="#{(bindings.TokenView1.estimatedRowCount > 0)?'lista':'boton'}"                             defaultFacet="conListado" id="s2">                    <f:facet name="lista">                        <af:listView value="#{bindings.TokenView1.collectionModel}" var="item"                                     emptyText="#{bindings.TokenView1.viewable ? 'No data to display.' : 'Access Denied.'}"                                     fetchSize="#{bindings.TokenView1.rangeSize}" id="lv12"                                     binding="#{BackingListaFrgHijo.lv12}">                            <af:listItem id="li1">                                <af:panelGroupLayout layout="horizontal" id="pgl1">                                    <af:image id="i1" source="../../Imagen-001/icons/3994434_email_at_email_address_contact_address_at_contact.svg"                                               shortDesc="image" rendered="#{item.bindings.Tipo.inputValue eq 'C'}"                                              styleClass="estilo32 estiloDarkBlue"/>                                    <af:image id="i2" source="../../Imagen-001/icons/703527_g_google 2015_google_new google.svg"                                              shortDesc="image" rendered="#{item.bindings.Tipo.inputValue eq 'G'}"                                              styleClass="estilo32 estiloDarkBlue"/>                                    <af:image id="i3" shortDesc="image"                                              source="../../Imagen-001/icons/367581_social_facebook.svg"                                              rendered="#{item.bindings.Tipo.inputValue eq 'F'}"                                              styleClass="estilo32 estiloDarkBlue"/>                                    <af:outputFormatted value="#{item.bindings.SocialNick.inputValue}" id="of4"/>                                                                        <af:link text="#{item.bindings.Correo.inputValue}" id="l1" rendered="#{item.bindings.Tipo.inputValue eq 'C'}"                                             action="Editar"/>                                    <af:outputFormatted value="#{item.bindings.Correo.inputValue}" id="of5" rendered="#{item.bindings.Tipo.inputValue ne 'C'}"/>                                                                                                            <af:button text="#{bnd.usuario_token_crear_token_btn}"                                               id="b2" rendered="#{item.bindings.Tipo.inputValue eq 'C'}"                                               immediate="true"                                               actionListener="#{BackingListaFrgHijo.actionListenerError}">                                        <af:setActionListener from="#{item.bindings.IdToken.inputValue}"                                                              to="#{sessionScope.idCodigo}"/>                                    </af:button>                                                                        <f:facet name="separator">                                        <af:spacer width="10" id="s1"/>                                    </f:facet>                                </af:panelGroupLayout>                            </af:listItem>                        </af:listView>                    </f:facet>                    <f:facet name="boton">                        <af:button text="#{bnd.usuario_token_listaFrg_boton_crear}" id="b1" action="Create"/>                    </f:facet>                </af:switcher>            </f:facet>            <f:facet name="elementoAcciones"/>        </af:pageTemplate>        <af:popup childCreation="deferred" autoCancel="disabled" id="p1" binding="#{BackingListaFrgHijo.popCrearClave}"                  contentDelivery="immediate">            <af:dialog id="d1" type="cancel" title="#{bnd.usuario_token_crear_token}">                <f:facet name="buttonBar">                    <af:button disabled="#{!bindings.administrativo_crearToken.enabled}" id="cb1"                               text="#{bnd.usuario_token_crear_token_btn}" action="#{BackingListaFrgHijo.actionCrearToken}"/>                </f:facet>                <af:panelBorderLayout id="pbl1">                    <af:panelHeader id="ph1" text="#{bnd.usuario_token_crear_token_descripcion}" messageType="warning">                        <af:outputText value="#{bnd.usuario_token_crear_token_advertencia}" id="ot1"/>                    </af:panelHeader>                </af:panelBorderLayout>            </af:dialog>        </af:popup>    </af:panelGroupLayout></jsp:root>
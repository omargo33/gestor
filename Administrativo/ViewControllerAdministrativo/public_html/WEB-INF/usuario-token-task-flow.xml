<?xml version="1.0" encoding="UTF-8" ?><adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">  <task-flow-definition id="usuario-token-task-flow">    <template-reference>      <document>/WEB-INF/plantilla/detalle-template.xml</document>      <id>detalle-template</id>    </template-reference>    <input-parameter-definition id="__1">      <name>pNick</name>      <value>#{pageFlowScope.pNick}</value>      <class>java.lang.String</class>    </input-parameter-definition>    <managed-bean id="__20">      <managed-bean-name>BackingListaFrgHijo</managed-bean-name>      <managed-bean-class>viewAdministrativo.usuario.token.ListaFrgHijoBean</managed-bean-class>      <managed-bean-scope>request</managed-bean-scope>    </managed-bean>    <view id="ListaFrg">      <page>/usuario/token/ListaFrg.jsff</page>    </view>    <method-call id="Create">      <method>#{bindings.CreateWithParams.execute}</method>      <outcome>        <fixed-outcome>Create</fixed-outcome>      </outcome>    </method-call>    <view id="ItemFrg">      <page>/usuario/token/ItemFrg.jsff</page>    </view>    <method-call id="Recargar">      <method>#{bindings.ejecutarIdEntidad.execute}</method>      <outcome>        <fixed-outcome>Recargar</fixed-outcome>      </outcome>    </method-call>    <use-page-fragments/>  </task-flow-definition></adfc-config>
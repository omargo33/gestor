<?xml version="1.0" encoding="UTF-8" ?><adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">  <task-flow-definition id="menu-permiso-detalle-task-flow">    <template-reference>      <document>/WEB-INF/plantilla/detalle-template.xml</document>      <id>detalle-template</id>    </template-reference>    <exception-handler>Error</exception-handler>    <method-call id="Recargar">      <method>#{bindings.ejecutarIdEntidad.execute}</method>      <outcome>        <fixed-outcome>Recargar</fixed-outcome>      </outcome>    </method-call>    <method-call id="Create">      <method>#{bindings.CreateWithParams.execute}</method>      <outcome>        <fixed-outcome>Create</fixed-outcome>      </outcome>    </method-call>    <view id="ListaFrg">      <page>/menu/permisos/ListaFrg.jsff</page>    </view>    <view id="ItemFrg">      <page>/menu/permisos/ItemFrg.jsff</page>    </view>    <use-page-fragments/>  </task-flow-definition></adfc-config>
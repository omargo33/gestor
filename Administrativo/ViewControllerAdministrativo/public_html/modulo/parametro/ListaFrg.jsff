<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core" xmlns:bl01="/baseLib01">    <af:pageTemplate viewId="/plantilla/listaFrg.jspx" id="pt1" value="#{bindings.ptb1}">        <f:attribute name="titulo" value="#{bnd.modulo_parametro_listaFrg_titulo}"/>        <f:attribute name="tituloDescripcion" value="#{bnd.modulo_parametro_listaFrg_descripcion}"/>        <f:facet name="elementoBusqueda"/>        <f:facet name="elementoCentro">            <af:table value="#{bindings.ParametroView1.collectionModel}" var="row"                      rows="#{bindings.ParametroView1.rangeSize}"                      emptyText="#{bindings.ParametroView1.viewable ? 'No data to display.' : 'Access Denied.'}"                      rowBandingInterval="0" selectedRowKeys="#{bindings.ParametroView1.collectionModel.selectedRow}"                      selectionListener="#{bindings.ParametroView1.collectionModel.makeCurrent}" rowSelection="single"                      fetchSize="#{bindings.ParametroView1.rangeSize}" id="t1" columnStretching="column:c5"                      scrollPolicy="page">                <?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.compreqsummary?><?audit suppress oracle.adf.faces.compreqsummary?><?audit suppress oracle.adf.faces.compreqsummary?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.compreqsummary?><?audit suppress oracle.adf.faces.tableneedsrowheader?>                <af:column headerText="#{bindings.ParametroView1.hints.IdParametro.label}" id="c1">                    <af:outputText value="#{row.IdParametro}"                                   shortDesc="#{bindings.ParametroView1.hints.IdParametro.tooltip}" id="ot1">                        <af:convertNumber groupingUsed="false"                                          pattern="#{bindings.ParametroView1.hints.IdParametro.format}"/>                    </af:outputText>                </af:column>                <af:column headerText="#{bindings.ParametroView1.hints.Indice.label}" id="c3">                    <af:outputText value="#{row.Indice}" shortDesc="#{bindings.ParametroView1.hints.Indice.tooltip}"                                   id="ot2"/>                </af:column>                <af:column headerText="#{bindings.ParametroView1.hints.Nombre.label}" id="c4">                    <af:link id="l1" action="Editar">                        <af:outputText value="#{row.Nombre}" shortDesc="#{bindings.ParametroView1.hints.Nombre.tooltip}"                                       id="ot3"/>                    </af:link>                </af:column>                <af:column headerText="#{bindings.ParametroView1.hints.Descripcion.label}" id="c5">                    <af:outputText value="#{row.Descripcion}"                                   shortDesc="#{bindings.ParametroView1.hints.Descripcion.tooltip}" id="ot4"/>                </af:column>                <af:column headerText="#{bindings.ParametroView1.hints.IdModulo.label}" id="c2">                    <af:selectOneChoice value="#{row.bindings.IdModulo.inputValue}"                                        label="#{row.bindings.IdModulo.label}"                                        required="#{bindings.ParametroView1.hints.IdModulo.mandatory}"                                        shortDesc="#{bindings.ParametroView1.hints.IdModulo.tooltip}" readOnly="true"                                        id="soc1">                        <f:selectItems value="#{row.bindings.IdModulo.items}" id="si1"/>                        <f:validator binding="#{row.bindings.IdModulo.validator}"/>                    </af:selectOneChoice>                </af:column>            </af:table>        </f:facet>        <f:facet name="elementoAcciones">            <bl01:botonesAtrasCrear id="bac1" atrasAccion="Inicio" crearAccion="Create"                                    crearTexto="#{bnd.menu_permiso_listaFrg_boton_crear}"                                    crearPermiso="#{pageFlowScope.hPermisoCrear}"/>        </f:facet>    </af:pageTemplate></jsp:root>
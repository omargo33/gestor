<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core" xmlns:bl01="/baseLib01">    <af:pageTemplate viewId="/plantilla/listaFrg.jspx" id="pt1" value="#{bindings.ptb1}">        <f:attribute name="titulo" value="#{bnd.informacion_listaFrg_titulo}"/>        <f:attribute name="tituloDescripcion" value="#{bnd.informacion_listaFrg_descripcion}"/>        <f:facet name="elementoBusqueda">            <af:query id="qryId1" value="#{bindings.InformacionViewCriteriaQuery.queryDescriptor}"                      model="#{bindings.InformacionViewCriteriaQuery.queryModel}"                      queryListener="#{bindings.InformacionViewCriteriaQuery.processQuery}"                      queryOperationListener="#{bindings.InformacionViewCriteriaQuery.processQueryOperation}"                      headerText="#{bnd.msg_busquea_titulo}" conjunctionReadOnly="true"                      searchButtonText="#{bnd.btn_buscar}" resetButtonText="#{bnd.btn_limpiar}" labelWidth="240"                      saveQueryMode="hidden" modeChangeVisible="false" disclosed="true" resultComponentId="::resId1"/>        </f:facet>        <f:facet name="elementoCentro">            <af:table value="#{bindings.InformacionView1.collectionModel}" var="row"                      rows="#{bindings.InformacionView1.rangeSize}"                      emptyText="#{bindings.InformacionView1.viewable ? 'No data to display.' : 'Access Denied.'}"                      rowBandingInterval="0" selectedRowKeys="#{bindings.InformacionView1.collectionModel.selectedRow}"                      selectionListener="#{bindings.InformacionView1.collectionModel.makeCurrent}" rowSelection="single"                      fetchSize="#{bindings.InformacionView1.rangeSize}" id="resId1" columnStretching="column:resId1c3"                      scrollPolicy="page">                <?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.compreqsummary?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.compreqsummary?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?><?audit suppress oracle.adf.faces.tableneedsrowheader?>                <af:column headerText="#{bindings.InformacionView1.hints.IdInformacion.label}" id="resId1c1">                    <af:outputText value="#{row.IdInformacion}"                                   shortDesc="#{bindings.InformacionView1.hints.IdInformacion.tooltip}" id="ot1">                        <af:convertNumber groupingUsed="false"                                          pattern="#{bindings.InformacionView1.hints.IdInformacion.format}"/>                    </af:outputText>                </af:column>                <af:column headerText="#{bindings.InformacionView1.hints.Nombre.label}" id="resId1c2">                    <af:link id="l1" action="Editar">                        <af:outputText value="#{row.Nombre}"                                       shortDesc="#{bindings.InformacionView1.hints.Nombre.tooltip}" id="ot2"/>                    </af:link>                </af:column>                <af:column headerText="#{bindings.InformacionView1.hints.Valor01.label}" id="resId1c3">                    <af:outputText value="#{row.Valor01}" shortDesc="#{bindings.InformacionView1.hints.Valor01.tooltip}"                                   id="ot3"/>                </af:column>                <af:column headerText="#{bindings.InformacionView1.hints.UsuarioFecha.label}" id="resId1c4">                    <af:outputText value="#{row.UsuarioFecha}"                                   shortDesc="#{bindings.InformacionView1.hints.UsuarioFecha.tooltip}" id="ot4">                        <af:convertDateTime pattern="#{bindings.InformacionView1.hints.UsuarioFecha.format}"/>                    </af:outputText>                </af:column>            </af:table>        </f:facet>        <f:facet name="elementoAcciones">            <bl01:botonesAtrasCrear id="bac1" atrasAccion="Inicio" crearAccion="Create"                                    crearTexto="#{bnd.informacion_listaFrg_boton_crear}"                                    crearPermiso="#{pageFlowScope.pPermisoCrear}"/>        </f:facet>    </af:pageTemplate></jsp:root>
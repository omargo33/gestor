<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core" xmlns:bl01="/baseLib01">    <af:panelGroupLayout id="pgl2">        <af:pageTemplate viewId="/plantilla/itemFrg.jspx" id="pt1">            <f:attribute name="titulo" value="#{bnd.informacion_itemFrg_titulo}"/>            <f:attribute name="tituloDescripcion" value="#{bnd.informacion_itemFrg_descripcion}"/>            <f:attribute name="permisoVerAuditoria" value="#{pageFlowScope.pPermisoVerAuditoria}"/>            <f:facet name="elementoCentro">                <af:panelFormLayout id="pfl1" labelWidth="140">                    <af:inputText value="#{bindings.Nombre.inputValue}" label="#{bindings.Nombre.hints.label}"                                  required="#{bindings.Nombre.hints.mandatory}"                                  columns="#{bindings.Nombre.hints.displayWidth}"                                  maximumLength="#{bindings.Nombre.hints.precision}"                                  shortDesc="#{bindings.Nombre.hints.tooltip}" id="it2">                        <f:validator binding="#{bindings.Nombre.validator}"/>                    </af:inputText>                    <af:inputText value="#{bindings.Valor01.inputValue}" label="#{bindings.Valor01.hints.label}"                                  required="#{bindings.Valor01.hints.mandatory}"                                  columns="#{bindings.Valor01.hints.displayWidth}"                                  maximumLength="#{bindings.Valor01.hints.precision}"                                  shortDesc="#{bindings.Valor01.hints.tooltip}" id="it3"                                  rows="#{bindings.Valor01.hints.displayHeight}">                        <f:validator binding="#{bindings.Valor01.validator}"/>                    </af:inputText>                    <af:inputText value="#{bindings.Valor02.inputValue}" label="#{bindings.Valor02.hints.label}"                                  required="#{bindings.Valor02.hints.mandatory}"                                  columns="#{bindings.Valor02.hints.displayWidth}"                                  maximumLength="#{bindings.Valor02.hints.precision}"                                  shortDesc="#{bindings.Valor02.hints.tooltip}" id="it4"                                  rows="#{bindings.Valor02.hints.displayHeight}">                        <f:validator binding="#{bindings.Valor02.validator}"/>                    </af:inputText>                    <f:facet name="footer">                        <bl01:botonesGuardarBorrar id="bgb1" actualizarAccion="#{BackingItemFrg.actionGuardar}"                                                   actualizarPermiso="#{pageFlowScope.pPermisoActualizar}"                                                   borrarAccion="#{BackingItemFrg.actionDelete}"                                                   borrarDisabled="#{bindings.IdInformacion.inputValue eq 0}"                                                   borrarPermiso="#{pageFlowScope.pPermisoBorrar}"/>                    </f:facet>                </af:panelFormLayout>            </f:facet>            <f:facet name="elementoCancelar">                <bl01:botonesCancelar id="bc1" inicioAccion="Inicio">                    <f:facet name="botonesAcciones"/>                </bl01:botonesCancelar>            </f:facet>            <f:facet name="elementoAuditoria">                <af:panelFormLayout id="pfl2" labelWidth="110">                    <f:facet name="footer"/>                    <af:inputText value="#{bindings.IdInformacion.inputValue}"                                  label="#{bindings.IdInformacion.hints.label}"                                  required="#{bindings.IdInformacion.hints.mandatory}"                                  columns="#{bindings.IdInformacion.hints.displayWidth}"                                  maximumLength="#{bindings.IdInformacion.hints.precision}"                                  shortDesc="#{bindings.IdInformacion.hints.tooltip}" id="it1">                        <f:validator binding="#{bindings.IdInformacion.validator}"/>                        <af:convertNumber groupingUsed="false" pattern="#{bindings.IdInformacion.format}"/>                    </af:inputText>                    <af:inputText value="#{bindings.Usuario.inputValue}" label="#{bindings.Usuario.hints.label}"                                  required="#{bindings.Usuario.hints.mandatory}"                                  columns="#{bindings.Usuario.hints.displayWidth}"                                  maximumLength="#{bindings.Usuario.hints.precision}"                                  shortDesc="#{bindings.Usuario.hints.tooltip}" id="it5">                        <f:validator binding="#{bindings.Usuario.validator}"/>                    </af:inputText>                    <af:inputDate value="#{bindings.UsuarioFecha.inputValue}"                                  label="#{bindings.UsuarioFecha.hints.label}"                                  required="#{bindings.UsuarioFecha.hints.mandatory}"                                  columns="#{bindings.UsuarioFecha.hints.displayWidth}"                                  shortDesc="#{bindings.UsuarioFecha.hints.tooltip}" id="id1">                        <f:validator binding="#{bindings.UsuarioFecha.validator}"/>                        <af:convertDateTime pattern="#{bindings.UsuarioFecha.format}"/>                    </af:inputDate>                    <af:inputText value="#{bindings.UsuarioPrograma.inputValue}"                                  label="#{bindings.UsuarioPrograma.hints.label}"                                  required="#{bindings.UsuarioPrograma.hints.mandatory}"                                  columns="#{bindings.UsuarioPrograma.hints.displayWidth}"                                  maximumLength="#{bindings.UsuarioPrograma.hints.precision}"                                  shortDesc="#{bindings.UsuarioPrograma.hints.tooltip}" id="it6">                        <f:validator binding="#{bindings.UsuarioPrograma.validator}"/>                    </af:inputText>                </af:panelFormLayout>            </f:facet>        </af:pageTemplate>    </af:panelGroupLayout></jsp:root>
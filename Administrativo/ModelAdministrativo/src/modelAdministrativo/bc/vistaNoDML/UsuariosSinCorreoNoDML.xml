<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd"><!----><ViewObject  xmlns="http://xmlns.oracle.com/bc4j"  Name="UsuariosSinCorreoNoDML"  Version="12.2.1.22.48"  InheritPersonalization="merge"  BindingStyle="JDBC"  CustomQuery="true"  FetchMode="FETCH_AS_NEEDED"  PageIterMode="Full"  AccessMode="DEFAULT">  <DesignTime>    <Attr Name="_isExpertMode" Value="true"/>  </DesignTime>  <Properties>    <SchemaBasedProperties>      <LABEL        ResId="modelAdministrativo.bc.vistaNoDML.UsuariosSinCorreoNoDML_LABEL"/>    </SchemaBasedProperties>  </Properties>  <SQLQuery><![CDATA[select (       select count(*)     from usuario u     where estado = 'A') usuarios,(    select count(*)     from token t      where t.id_usuario in     (        select u.id_usuario         from usuario u         where estado = 'A'    )    and t.tipo = 'C') usuarios_correo]]></SQLQuery>  <ViewAttribute    Name="Usuarios"    IsUpdateable="false"    IsPersistent="false"    PrecisionRule="true"    Type="java.math.BigDecimal"    ColumnType="BIGINT"    AliasName="usuarios"    Expression="usuarios"    SQLType="NUMERIC">    <DesignTime>      <Attr Name="_DisplaySize" Value="21"/>    </DesignTime>  </ViewAttribute>  <ViewAttribute    Name="UsuariosCorreo"    IsUpdateable="false"    IsPersistent="false"    PrecisionRule="true"    Type="java.math.BigDecimal"    ColumnType="BIGINT"    AliasName="usuarios_correo"    Expression="usuarios_correo"    SQLType="NUMERIC">    <DesignTime>      <Attr Name="_DisplaySize" Value="21"/>    </DesignTime>  </ViewAttribute>  <AttrArray Name="KeyAttributes">    <Item Value="Usuarios"/>  </AttrArray>  <ResourceBundle>    <PropertiesBundle      PropertiesFile="modelAdministrativo.ModelAdministrativoBundle"/>  </ResourceBundle></ViewObject>